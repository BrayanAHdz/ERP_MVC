                        </div>
                        <label>Vulnerabilidad</label>
                        <div class="progress">
                            <div class="progress-bar" role="progressbar" style="width: @progress(moduleA[5],2)%;" aria-valuenow="@progress(moduleA[5],2)" aria-valuemin="0" aria-valuemax="100">@progress(moduleA[5], 2)%</div>
                        </div>
                    </div>
                </div>
                <div class="col-sm-4">
                    <label>Extroversión</label><br /><br />
                    <div>
                        <label>Cordialidad</label>
                        <div class="progress">
                            <div class="progress-bar" role="progressbar" style="width: @progress(moduleB[0],2)%;" aria-valuenow="@progress(moduleB[0],2)" aria-valuemin="0" aria-valuemax="100">@progress(moduleB[0], 2)%</div>
                        </div>
                        <label>Sociabilidad</label>
