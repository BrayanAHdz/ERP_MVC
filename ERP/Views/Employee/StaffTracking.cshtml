                            <div class="progress-bar" role="progressbar" style="width: @progress(moduleA[1],2)%;" aria-valuenow="@progress(moduleA[1],2)" aria-valuemin="0" aria-valuemax="100">@progress(moduleA[1], 2)%</div>
                        </div>
                        <label>Depresión</label>
                        <div class="progress">
